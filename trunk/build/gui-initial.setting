<?xml version="1.0" encoding="UTF-8"?>

<!-- The whole goal here is to enable filter-out-tasks-with-no-descriptions -->

<root>
  <setting name="task-tab">
    <setting name="basic-project-and-task-filter">
      <setting name="filter-out-tasks-with-no-description" value="true"/>
      <setting name="filtered-out-projects"/>
      <setting name="filtered-out-tasks"/>
    </setting>
  </setting>
</root>
